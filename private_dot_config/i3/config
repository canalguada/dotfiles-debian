# set_from_resource $<name> <resource_name> <fallback> {{{
set_from_resource $primary	 i3wm.primary		#315bef
set_from_resource $secondary i3wm.secondary #39cccc
set_from_resource $darkbg		 i3wm.darkbg		#151515
set_from_resource $lightbg 	 i3wm.lightbg		#2a2a2a
set_from_resource $lightfg	 i3wm.lightfg		#dedede
set_from_resource $grey			 i3wm.color8		#555753
set_from_resource $alert		 i3wm.alert			#f012be
set_from_resource $silver		 i3wm.silver		#dddddd
# xrdb
# class                 border bground text indicator child_border
client.focused          $primary $primary	$lightfg $primary	$primary
client.focused_inactive $grey		 $grey		$lightfg $grey		$grey
client.unfocused        $darkbg	 $darkbg	$silver	 $darkbg	$darkbg
client.urgent           $grey 	 $alert		$lightfg $alert		$alert
client.placeholder      $darkbg	 $darkbg	$lightfg $darkbg 	$darkbg
client.background       $lightfg
