# Use pactl to adjust volume in PulseAudio.  {{{
#set $refresh_i3status killall -SIGUSR1 i3status
#set $refresh_status killall -SIGUSR1 py3status
set $refresh_status true
set $raise_volume pactl set-sink-volume @DEFAULT_SINK@ +5%
set $lower_volume pactl set-sink-volume @DEFAULT_SINK@ -5%
set $mute pactl set-sink-mute @DEFAULT_SINK@ toggle
# set $raise_volume pavolume up
# set $lower_volume pavolume down
# set $mute pavolume mute
bindsym XF86AudioRaiseVolume \
exec --no-startup-id $raise_volume && $refresh_status
bindsym XF86AudioLowerVolume \
exec --no-startup-id $lower_volume && $refresh_status
bindsym XF86AudioMute \
exec --no-startup-id $mute && $refresh_status
set $mic_mute pactl set-source-mute @DEFAULT_SOURCE@ toggle
bindsym XF86AudioMicMute \
exec --no-startup-id $mic_mute && $refresh_status
# }}}
